{
  "name": "@aiready/cli",
  "version": "0.7.17",
  "description": "Unified CLI for AIReady analysis tools",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "aiready": "./dist/cli.js"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "@aiready/pattern-detect": "0.9.19",
    "@aiready/core": "0.7.10",
    "@aiready/context-analyzer": "0.7.15",
    "@aiready/consistency": "0.6.13"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "@types/node": "^20.0.0"
  },
  "keywords": [
    "aiready",
    "cli",
    "ai-readiness",
    "code-analysis"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caopengau/aiready-cli.git"
  },
  "homepage": "https://github.com/caopengau/aiready-cli",
  "bugs": {
    "url": "https://github.com/caopengau/aiready-cli/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  }
}